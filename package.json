{
    "name": "n8n-nodes-qonto-api",
    "version": "0.1.0",
    "description": "n8n node for Qonto banking API - manage transactions, transfers, beneficiaries, and more",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package",
        "qonto",
        "banking",
        "api",
        "finance",
        "transactions"
    ],
    "author": {
        "name": "Jan Thiel",
        "email": "jan@hive-it.de"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JanThiel/n8n-nodes-qonto.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/QontoApi.credentials.js",
            "dist/credentials/QontoOAuth2Api.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Qonto/Qonto.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
